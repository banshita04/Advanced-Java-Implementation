package programming;

public class RunningThreads {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		Runnable runnable = new Runnable() {
			
			@Override
			public void run() {
				// TODO Auto-generated method stub
				for(int i=0; i < 10000; i++) {
					System.out.println(Thread.currentThread().getId()+" : "+ i);
				}
			}
		};
	}

}
